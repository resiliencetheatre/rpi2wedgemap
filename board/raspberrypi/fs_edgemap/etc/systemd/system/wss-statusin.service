[Unit]
Description=gwsocket for meshtastic status messages (SSL/8995)
After=multi-user.target
Conflicts=ws-statusin.service

[Service]
WorkingDirectory=/opt/edgemap
KillMode=process
ExecStart=gwsocket -p 8995 --pipein=/tmp/statusin --ssl-cert=/etc/apache2/edgemap.crt --ssl-key=/etc/apache2/edgemap.key
Restart=always
RestartSec=1
TimeoutStartSec=5

[Install]
WantedBy=multi-user.target
